{
  "expo": {
    "name": "thinky",
    "slug": "thinky",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/thinky-ios-logo.png",
    "scheme": "thinky",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sandeepamarnath.thinky",
      "icon": "./assets/icons/thinky-ios-logo.png"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/icons/thinky-android-logo.png",
        "backgroundImage": "./assets/icons/thinky-android-logo.png",
        "monochromeImage": "./assets/icons/thinky-android-logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.sandeepamarnath.thinky"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/icons/thinky-ios-logo.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-font"
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/thinky-android-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "67c07615-375e-4c49-9e0b-dcd7735c40f6"
      }
    },
    "owner": "sandeepamarnath"
  }
}
